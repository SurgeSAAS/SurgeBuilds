{
  "outputDirectory": ".",
  "rewrites": [
    {
      "source": "/:path*",
      "has": [{ "type": "host", "value": "shopwithluca.surgesites.net" }],
      "destination": "/clients/shopwithluca/index.html"
    },
    {
      "source": "/:path*",
      "has": [{ "type": "host", "value": "admin.surgesites.net" }],
      "destination": "/admin/index.html"
    },
    {
      "source": "/:path*",
      "has": [{ "type": "host", "value": "admin.shopwithluca.surgesites.net" }],
      "destination": "/clients/shopwithluca/admin/index.html"
    }
  ]
}
